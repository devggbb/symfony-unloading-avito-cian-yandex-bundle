{
    "name": "ggbb/symfony-uploading-avito-cian-yandex-bundle",
    "description": "Upload bundle for unloading in avito, cian, yandex",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/yaml": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/validator": "^6.3",
        "doctrine/orm": "^2.15",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Ggbb\\SymfonyUploadingAvitoCianYandexBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ggbb",
            "email": "devggbb@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
